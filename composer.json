{
    "name": "wlib/dibox",
    "description": "DiBox is a Dependency Injection Container.",
    "type": "library",
    "keywords": ["container", "psr11", "dependency", "injection", "di", "wlib"],
    "license": "CECILL-2.1",
    "require": {
        "php": ">=7.4.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "wlib\\Di\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Cédric Ducarre",
            "email": "samray1024@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
